.stuffContent
  div.row
    .searchForm.small-12.columns 
      form(role='form', novalidate)
        .form-group
          label(for='url') URL
          input(ng-model='search.mainUrl', type='text', autofocus)
        .form-group
          label(for='depth') Depth
          input(ng-model='search.depth', type='text', ng-value='1', autofocus)
      button.button.secondary.success.alert(ng-click='search()') Search